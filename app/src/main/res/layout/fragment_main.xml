<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             tools:context=".FragmentMain">
    <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="120dp">
            <ImageView
                    android:src="@drawable/ic_flight_takeoff_black_24dp"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent" android:id="@+id/imageView5" android:layout_weight="1"/>
            <ImageView
                    android:src="@drawable/ic_people_black_24dp"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent" android:id="@+id/imageView4" android:layout_weight="1"/>
        </LinearLayout>
        <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="120dp">
            <ImageView
                    android:src="@drawable/ic_wc_black_24dp"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent" android:id="@+id/imageView7" android:layout_weight="1"/>
            <ImageView
                    android:src="@drawable/ic_local_hotel_black_24dp"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent" android:id="@+id/imageView6" android:layout_weight="1"/>
            <ImageView
                    android:src="@drawable/ic_pregnant_woman_black_24dp"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent" android:id="@+id/imageView8" android:layout_weight="1"/>
        </LinearLayout>
        <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="120dp">
            <ImageView
                    android:src="@drawable/ic_child_care_black_24dp"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent" android:id="@+id/imageView" android:layout_weight="1"/>
            <ImageView
                    android:src="@drawable/ic_person_black_24dp"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent" android:id="@+id/imageView3" android:layout_weight="1"/>
        </LinearLayout>
    </LinearLayout>
</FrameLayout>